<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>

<!-- fx:controller="di.unito.it.prog3.client.controllers.ToolbarController" -->
<HBox id="toolbar" spacing="15.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" >
   <children>
      <Button contentDisplay="TOP" mnemonicParsing="false" text="Write">
         <graphic>
            <ImageView fitHeight="30.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@893229-email/png/041-email-2.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Button contentDisplay="TOP" mnemonicParsing="false" text="Forward">
         <graphic>
            <ImageView fitHeight="30.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@893229-email/png/010-email-25.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Button contentDisplay="TOP" mnemonicParsing="false" text="Reply">
         <graphic>
            <ImageView fitHeight="30.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@893229-email/png/008-reply.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Button contentDisplay="TOP" mnemonicParsing="false" text="Reply All">
         <graphic>
            <ImageView fitHeight="30.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@893229-email/png/009-reply-all.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Button contentDisplay="TOP" disable="true" mnemonicParsing="false" text="Send">
         <graphic>
            <ImageView fitHeight="30.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@893229-email/png/006-email-27.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Button contentDisplay="TOP" mnemonicParsing="false" text="Delete">
         <graphic>
            <ImageView fitHeight="30.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@893229-email/png/024-mailbox-2.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
   </children>
   <padding>
      <Insets bottom="7.0" left="15.0" right="15.0" top="15.0" />
   </padding>
</HBox>
